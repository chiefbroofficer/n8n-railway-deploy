{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": "n8n start",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 3,
    "healthcheckPath": "/healthz",
    "healthcheckTimeout": 5,
    "region": "us-west1"
  },
  "services": [
    {
      "name": "n8n",
      "icon": "ðŸ¤–",
      "environments": {
        "production": {
          "build": {
            "builder": "DOCKERFILE"
          },
          "deploy": {
            "numReplicas": 1
          }
        }
      }
    }
  ]
}